    "@multiversx/sdk-dapp": "2.19.2",
    "@react-three/fiber": "8.13.6",
    "@reduxjs/toolkit": "1.9.5",
    "anchorme": "2.1.2",
    "axios": "0.24.0",
    "bech32": "2.0.0",
    "bignumber.js": "9.1.1",
    "bootstrap": "5.3.1",
    "classnames": "2.3.2",
    "file-saver": "2.0.5",
    "lodash.clonedeep": "4.5.0",
    "moment": "2.29.4",
    "numeral": "2.0.6",
    "react": "18.2.0",
    "react-anchorme": "2.2.1",
    "react-bootstrap": "2.8.0",
    "react-datepicker": "4.16.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-redux": "8.1.2",
    "react-router-dom": "6.15.0",
    "react-select": "5.7.4",
    "react-select-async-paginate": "0.7.3",
    "react-simple-maps": "3.0.0",
    "react-syntax-highlighter": "15.5.0",
    "recharts": "2.7.3",
    "redux-persist": "6.0.0",
    "three": "0.155.0",
    "three-stdlib": "2.24.2",
    "ua-parser-js": "1.0.35",
    "yup": "0.32.11"
  },
  "scripts": {
    "prepare": "./prepare-pro-icons.sh",
    "start": "VITE_APP_CACHE_BUST=$(git describe --always) vite",
    "start-testnet": "yarn run copy-testnet-config & yarn run start",
    "start-internal": "yarn run copy-internal-config & yarn run start",
    "start-devnet": "yarn run copy-devnet-config & yarn run start",
    "start-mainnet": "yarn run copy-mainnet-config & yarn run start",
    "build-mainnet": "yarn run copy-mainnet-config & VITE_APP_SHARE_PREFIX= VITE_APP_GA_ID=***REMOVED*** yarn run build",
    "build-staging": "yarn run copy-staging-config & VITE_APP_SHARE_PREFIX= VITE_APP_GA_ID=***REMOVED*** VITE_APP_IS_STAGING=true yarn run build",
    "build-testnet": "yarn run copy-testnet-config & VITE_APP_SHARE_PREFIX=testnet- VITE_APP_GA_ID=G-EFN7QYSMQS yarn run build",
    "build-internal": "yarn run copy-internal-config & VITE_APP_SHARE_PREFIX=internal- VITE_APP_GA_ID=G-EFN7QYSMQS yarn run build",
    "build-next": "yarn run copy-next-config & VITE_APP_SHARE_PREFIX=next- VITE_APP_GA_ID=G-EFN7QYSMQS yarn run build",
    "build-devnet": "yarn run copy-devnet-config & VITE_APP_SHARE_PREFIX=devnet- VITE_APP_GA_ID=G-QE38EW59B8 yarn run build",
    "build": "VITE_APP_CACHE_BUST=$(git describe --always) tsc && vite build && yarn version:make",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "copy-mainnet-config": "run-script-os",
    "copy-staging-config": "run-script-os",
    "copy-testnet-config": "run-script-os",
    "copy-devnet-config": "run-script-os",
    "copy-internal-config": "run-script-os",
    "copy-next-config": "run-script-os",
    "copy-mainnet-config:nix": "cp ./src/config/config.mainnet.ts ./src/config/index.ts",
    "copy-staging-config:nix": "cp ./src/config/config.staging.ts ./src/config/index.ts",
    "copy-testnet-config:nix": "cp ./src/config/config.testnet.ts ./src/config/index.ts",
    "copy-devnet-config:nix": "cp ./src/config/config.devnet.ts ./src/config/index.ts",
    "copy-internal-config:nix": "cp ./src/config/config.internal.ts ./src/config/index.ts",
    "copy-next-config:nix": "cp ./src/config/config.next.ts ./src/config/index.ts",
    "copy-mainnet-config:windows": "copy .\\src\\config\\config.mainnet.ts .\\src\\config\\index.ts",
    "copy-staging-config:windows": "copy .\\src\\config\\config.staging.ts .\\src\\config\\index.ts",
    "copy-testnet-config:windows": "copy .\\src\\config\\config.testnet.ts .\\src\\config\\index.ts",
    "copy-devnet-config:windows": "copy .\\src\\config\\config.devnet.ts .\\src\\config\\index.ts",
    "copy-internal-config:windows": "copy .\\src\\config\\config.internal.ts .\\src\\config\\index.ts",
    "copy-next-config:windows": "copy .\\src\\config\\config.next.ts .\\src\\config\\index.ts",
    "version:make": "printf '{\"shortSha\":\"%s\"}' $(git describe --always) > build/version.json",
    "prepare-free-icons": "./prepare-free-icons.sh",
    "prepare-pro-icons": "./prepare-pro-icons.sh"
  },
  "devDependencies": {
    "@types/file-saver": "2.0.5",
    "@types/lodash.clonedeep": "4.5.7",
    "@types/numeral": "2.0.2",
    "@types/react": "18.2.20",
    "@types/react-bootstrap": "0.32.32",
    "@types/react-datepicker": "4.15.0",
    "@types/react-dom": "18.2.7",
    "@types/react-helmet": "6.1.6",
    "@types/react-simple-maps": "3.0.0",
    "@types/react-syntax-highlighter": "15.5.7",
    "@types/three": "0.155.0",
    "@types/ua-parser-js": "0.7.36",
    "@vitejs/plugin-react-swc": "3.3.2",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "3.0.0",
    "run-script-os": "1.1.5",
    "sass": "1.62.1",
    "typescript": "5.1.6",
    "vite": "4.4.9",
    "vite-plugin-mkcert": "1.16.0",
    "vite-plugin-node-polyfills": "0.9.0",
    "vite-plugin-svgr": "3.2.0",
    "vite-tsconfig-paths": "4.2.0"
  }
}
