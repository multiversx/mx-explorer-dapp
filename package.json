{
  "name": "mx-explorer-dapp",
  "version": "2.0.0",
  "private": true,
  "author": "MultiversX",
  "repository": "multiversx/mx-explorer-dapp",
  "keywords": [
    "multiversx",
    "explorer"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.2.1",
    "@fortawesome/free-brands-svg-icons": "6.2.1",
    "@fortawesome/pro-duotone-svg-icons": "6.2.1",
    "@fortawesome/pro-light-svg-icons": "6.2.1",
    "@fortawesome/pro-regular-svg-icons": "6.2.1",
    "@fortawesome/pro-solid-svg-icons": "6.2.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@multiversx/sdk-dapp": "2.3.0",
    "@reduxjs/toolkit": "1.8.2",
    "anchorme": "2.1.2",
    "axios": "0.24.0",
    "bech32": "2.0.0",
    "bignumber.js": "9.1.1",
    "bootstrap": "5.2.3",
    "classnames": "2.3.2",
    "file-saver": "2.0.5",
    "moment": "2.29.4",
    "numeral": "2.0.6",
    "react": "18.2.0",
    "react-anchorme": "2.2.1",
    "react-bootstrap": "2.7.0",
    "react-datepicker": "4.8.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-redux": "8.0.2",
    "react-router-dom": "6.7.0",
    "react-scripts": "4.0.3",
    "react-select": "5.7.0",
    "react-simple-maps": "3.0.0",
    "recharts": "2.3.2",
    "redux-persist": "6.0.0",
    "yup": "0.32.11"
  },
  "scripts": {
    "start": "REACT_APP_CACHE_BUST=$(git describe --always) HTTPS=true PORT=3001 react-scripts start",
    "start-testnet": "npm run copy-testnet-config & REACT_APP_THEME=testnet npm run start",
    "start-internal": "npm run copy-internal-config & REACT_APP_THEME=testnet npm run start",
    "start-devnet": "npm run copy-devnet-config & REACT_APP_THEME=testnet npm run start",
    "start-mainnet": "npm run copy-mainnet-config & REACT_APP_THEME=light npm run start",
    "build-mainnet": "npm run copy-mainnet-config & REACT_APP_SHARE_PREFIX= REACT_APP_GA_ID=***REMOVED*** REACT_APP_THEME=light npm run build",
    "build-staging": "npm run copy-staging-config & REACT_APP_SHARE_PREFIX= REACT_APP_GA_ID=***REMOVED*** REACT_APP_THEME=light REACT_APP_IS_STAGING=true npm run build",
    "build-testnet": "npm run copy-testnet-config & REACT_APP_SHARE_PREFIX=testnet- REACT_APP_GA_ID=G-EFN7QYSMQS REACT_APP_THEME=testnet npm run build",
    "build-internal": "npm run copy-internal-config & REACT_APP_SHARE_PREFIX=internal- REACT_APP_GA_ID=G-EFN7QYSMQS REACT_APP_THEME=testnet npm run build",
    "build-devnet": "npm run copy-devnet-config & REACT_APP_SHARE_PREFIX=devnet- REACT_APP_GA_ID=G-QE38EW59B8 REACT_APP_THEME=testnet npm run build",
    "build": "GENERATE_SOURCEMAP=false REACT_APP_CACHE_BUST=$(git describe --always) react-scripts build",
    "copy-mainnet-config": "cp ./src/config/config.mainnet.ts ./src/config/index.ts",
    "copy-staging-config": "cp ./src/config/config.staging.ts ./src/config/index.ts",
    "copy-testnet-config": "cp ./src/config/config.testnet.ts ./src/config/index.ts",
    "copy-internal-config": "cp ./src/config/config.internal.ts ./src/config/index.ts",
    "copy-devnet-config": "cp ./src/config/config.devnet.ts ./src/config/index.ts",
    "postbuild": "npm run create-version-file",
    "create-version-file": "git describe --always | sed 's/^/\"/;s/$/\"/' > build/version.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --fix --ext js,ts,tsx src"
  },
  "jest": {
    "moduleNameMapper": {
      "@multiversx/sdk-dapp/(.*)": "<rootDir>/node_modules/@multiversx/sdk-dapp/__commonjs/$1.js"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "resolutions": {
    "tslib": "^2.3.0",
    "react-error-overlay": "6.0.9"
  },
  "overrides": {
    "react-error-overlay": "6.0.9"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-common-types": "6.2.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/file-saver": "2.0.5",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-datepicker": "4.8.0",
    "@types/react-dom": "18.0.10",
    "@types/react-bootstrap": "0.32.32",
    "@types/react-helmet": "6.1.6",
    "@types/react-simple-maps": "3.0.0",
    "@types/numeral": "2.0.2",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-react": "1.1.7",
    "eslint-config-react-app": "7.0.1",
    "eslint-config-standard": "17.0.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "2.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-overlay": "6.0.9",
    "sass": "1.57.1",
    "typescript": "4.9.4"
  }
}
