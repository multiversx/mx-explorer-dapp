{
  "name": "blockchain-explorer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.28",
    "@fortawesome/pro-light-svg-icons": "5.14.0",
    "@fortawesome/pro-regular-svg-icons": "5.14.0",
    "@fortawesome/pro-solid-svg-icons": "5.14.0",
    "@fortawesome/react-fontawesome": "0.1.9",
    "@types/axios": "0.14.0",
    "@types/bech32": "1.1.2",
    "@types/chart.js": "2.9.27",
    "@types/jest": "24.0.19",
    "@types/leaflet": "1.5.12",
    "@types/moment": "2.13.0",
    "@types/node": "14.0.13",
    "@types/node-sass": "4.11.1",
    "@types/react": "16.9.36",
    "@types/react-bootstrap": "1.0.1",
    "@types/react-dom": "16.9.8",
    "@types/react-leaflet": "2.5.1",
    "@types/react-router-dom": "5.1.5",
    "@types/yup": "0.29.3",
    "axios": "0.19.2",
    "bech32": "1.1.4",
    "bignumber.js": "9.0.0",
    "bootstrap": "4.5.0",
    "chart.js": "2.9.4",
    "leaflet": "1.6.0",
    "moment": "2.26.0",
    "node-sass": "4.14.1",
    "react": "16.13.1",
    "react-bootstrap": "1.0.1",
    "react-dom": "16.13.1",
    "react-leaflet": "2.7.0",
    "react-leaflet-control": "2.1.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "shards-ui": "3.0.0",
    "typescript": "3.9.5",
    "yup": "0.29.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build-mainnet": "npm run copy-mainnet-config & REACT_APP_SHARE_PREFIX= REACT_APP_GA_ID=UA-140910543-1 npm run build",
    "build-testnet": "npm run copy-testnet-config & REACT_APP_SHARE_PREFIX=testnet- REACT_APP_GA_ID=UA-129278450-2 npm run build",
    "build-internal": "npm run copy-internal-config & REACT_APP_SHARE_PREFIX=internal- npm run build",
    "build": "REACT_APP_CACHE_BUST=$(git describe --always) react-scripts build",
    "copy-mainnet-config": "cp ./config.mainnet.js ./public/config.js",
    "copy-testnet-config": "cp ./config.testnet.js ./public/config.js",
    "copy-internal-config": "cp ./config.internal.js ./public/config.js",
    "postbuild": "npm run compile",
    "compile-light": "node-sass src/assets/styles/light.scss build/light.css",
    "compile-dark": "node-sass src/assets/styles/dark.scss build/dark.css",
    "compile-testnet": "node-sass src/assets/styles/testnet.scss build/testnet.css",
    "compile": "npm run compile-light & npm run compile-dark & npm run compile-testnet",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "husky": {
    "hooks": {
      "pre-push": "CI=true npm run test --runInBand"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fortawesome/fontawesome-common-types": "0.2.30",
    "@hot-loader/react-dom": "16.13.0",
    "@testing-library/jest-dom": "4.2.0",
    "@testing-library/react": "9.3.0",
    "@welldone-software/why-did-you-render": "4.2.5",
    "husky": "4.2.5",
    "jest-canvas-mock": "2.3.0",
    "prettier": "2.0.5",
    "react-app-rewire-hot-loader": "2.0.1",
    "react-app-rewired": "2.1.6",
    "react-hot-loader": "4.12.21",
    "reinspect": "1.0.2",
    "tslint": "6.1.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "tslint-react": "5.0.0"
  }
}
